{
  "name": "ai-app",
  "version": "1.0.0",
  "description": "Boilerplate for Typescript Node application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "cli": "ts-node src/cli.ts",
    "dev": "tsc --project tsconfig.json --watch & NODE_ENV=development nodemon dist",
    "run-job-runner": "ts-node --transpile-only ./src/index-job-runner.ts",
    "prestart": "npm run build",
    "start": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "test-api-lambda": "ts-node --transpile-only ./src/test-api-lambda.ts",
    "test-job-runner": "ts-node --transpile-only ./src/test-job-runner.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenley/ai-app"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.980.0",
    "@aws-sdk/client-bedrock-runtime": "^3.971.0",
    "@aws-sdk/client-dynamodb": "^3.975.0",
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/client-ses": "^3.982.0",
    "@aws-sdk/client-sfn": "^3.971.0",
    "@aws-sdk/client-sqs": "^3.975.0",
    "@aws-sdk/client-ssm": "^3.971.0",
    "@aws-sdk/lib-storage": "^3.971.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-jwt-verify": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.1",
    "openai": "^6.16.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
